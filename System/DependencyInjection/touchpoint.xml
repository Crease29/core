<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Versioning -->
        <service alias="Shopware\System\Touchpoint\TouchpointRepository.v1" id="Shopware\System\Touchpoint\TouchpointRepository" public="true" />

         <service class="Shopware\System\Touchpoint\TouchpointDefinition" id="Shopware\System\Touchpoint\TouchpointDefinition" >
            <tag name="shopware.entity.definition" entity="application" />
        </service>

        <service class="Shopware\System\Touchpoint\TouchpointRepository" id="Shopware\System\Touchpoint\TouchpointRepository.v1" public="true">
          <argument id="Shopware\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                