<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Versioning -->
        <service alias="Shopware\System\Tax\Repository\TaxRepository.v1" id="Shopware\System\Tax\Repository\TaxRepository" public="true"/>
        <service alias="Shopware\System\Tax\Repository\TaxAreaRuleRepository.v1" id="Shopware\System\Tax\Repository\TaxAreaRuleRepository" public="true"/>
        <service alias="Shopware\System\Tax\Repository\TaxAreaRuleTranslationRepository.v1" id="Shopware\System\Tax\Repository\TaxAreaRuleTranslationRepository" public="true"/>


        <service class="Shopware\System\Tax\Definition\TaxDefinition" id="Shopware\System\Tax\Definition\TaxDefinition" >
            <tag name="shopware.entity.definition" entity="tax" />
        </service>
        <service class="Shopware\System\Tax\Repository\TaxRepository" id="Shopware\System\Tax\Repository\TaxRepository.v1" public="true">
          <argument id="Shopware\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Version\VersionManager" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\System\Tax\Definition\TaxAreaRuleDefinition" id="Shopware\System\Tax\Definition\TaxAreaRuleDefinition" >
            <tag name="shopware.entity.definition" entity="tax_area_rule" />
        </service>
        <service class="Shopware\System\Tax\Repository\TaxAreaRuleRepository" id="Shopware\System\Tax\Repository\TaxAreaRuleRepository.v1" public="true">
          <argument id="Shopware\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Version\VersionManager" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\System\Tax\Definition\TaxAreaRuleTranslationDefinition" id="Shopware\System\Tax\Definition\TaxAreaRuleTranslationDefinition" >
            <tag name="shopware.entity.definition" entity="tax_area_rule_translation" />
        </service>
        <service class="Shopware\System\Tax\Repository\TaxAreaRuleTranslationRepository" id="Shopware\System\Tax\Repository\TaxAreaRuleTranslationRepository.v1" public="true">
          <argument id="Shopware\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Version\VersionManager" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                