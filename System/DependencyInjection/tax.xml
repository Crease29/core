<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Core\System\Tax\TaxDefinition" id="Shopware\Core\System\Tax\TaxDefinition">
            <tag name="shopware.entity.definition" entity="tax"/>
        </service>

        <service class="Shopware\Core\System\Tax\Aggregate\TaxAreaRule\TaxAreaRuleDefinition" id="Shopware\Core\System\Tax\Aggregate\TaxAreaRule\TaxAreaRuleDefinition">
            <tag name="shopware.entity.definition" entity="tax_area_rule"/>
        </service>

        <service class="Shopware\Core\System\Tax\Aggregate\TaxAreaRuleTranslation\TaxAreaRuleTranslationDefinition" id="Shopware\Core\System\Tax\Aggregate\TaxAreaRuleTranslation\TaxAreaRuleTranslationDefinition">
            <tag name="shopware.entity.definition" entity="tax_area_rule_translation"/>
        </service>
    </services>
</container>                
                