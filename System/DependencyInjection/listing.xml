<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Versioning -->
        <service alias="Shopware\Core\System\Listing\ListingFacetRepository.v1" id="Shopware\Core\System\Listing\ListingFacetRepository" public="true"/>
        <service alias="Shopware\Core\System\Listing\ListingSortingRepository.v1" id="Shopware\Core\System\Listing\ListingSortingRepository" public="true"/>
        <service alias="Shopware\Core\System\Listing\Aggregate\ListingFacetTranslation\ListingFacetTranslationRepository.v1" id="Shopware\Core\System\Listing\Aggregate\ListingFacetTranslation\ListingFacetTranslationRepository" public="true"/>
        <service alias="Shopware\Core\System\Listing\Aggregate\ListingSortingTranslation\ListingSortingTranslationRepository.v1" id="Shopware\Core\System\Listing\Aggregate\ListingSortingTranslation\ListingSortingTranslationRepository" public="true"/>

        <service class="Shopware\Core\System\Listing\ListingFacetDefinition" id="Shopware\Core\System\Listing\ListingFacetDefinition" >
            <tag name="shopware.entity.definition" entity="listing_facet" />
        </service>
        <service class="Shopware\Core\System\Listing\ListingFacetRepository" id="Shopware\Core\System\Listing\ListingFacetRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Core\System\Listing\Definition\ListingFacetTranslationDefinition" id="Shopware\Core\System\Listing\Definition\ListingFacetTranslationDefinition" >
            <tag name="shopware.entity.definition" entity="listing_facet_translation" />
        </service>
        <service class="Shopware\Core\System\Listing\Aggregate\ListingFacetTranslation\ListingFacetTranslationRepository" id="Shopware\Core\System\Listing\Aggregate\ListingFacetTranslation\ListingFacetTranslationRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Core\System\Listing\ListingSortingDefinition" id="Shopware\Core\System\Listing\ListingSortingDefinition" >
            <tag name="shopware.entity.definition" entity="listing_sorting" />
        </service>
        <service class="Shopware\Core\System\Listing\ListingSortingRepository" id="Shopware\Core\System\Listing\ListingSortingRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Core\System\Listing\Definition\ListingSortingTranslationDefinition" id="Shopware\Core\System\Listing\Definition\ListingSortingTranslationDefinition" >
            <tag name="shopware.entity.definition" entity="listing_sorting_translation" />
        </service>
        <service class="Shopware\Core\System\Listing\Aggregate\ListingSortingTranslation\ListingSortingTranslationRepository" id="Shopware\Core\System\Listing\Aggregate\ListingSortingTranslation\ListingSortingTranslationRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                