<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Core\Content\Catalog\CatalogDefinition" id="Shopware\Core\Content\Catalog\CatalogDefinition">
            <tag name="shopware.entity.definition" entity="catalog"/>
        </service>

        <service class="Shopware\Core\Content\Catalog\Aggregate\CatalogTranslation\CatalogTranslationDefinition" id="Shopware\Core\Content\Catalog\Aggregate\CatalogTranslation\CatalogTranslationDefinition">
            <tag name="shopware.entity.definition" entity="catalog_translation"/>
        </service>
    </services>
</container>                
                