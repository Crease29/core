<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Versioning -->
        <service alias="Shopware\Core\Content\Configuration\ConfigurationGroupRepository.v1" id="Shopware\Core\Content\Configuration\ConfigurationGroupRepository" public="true" />
        <service alias="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOption\ConfigurationGroupOptionRepository.v1" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOption\ConfigurationGroupOptionRepository" public="true" />
        <service alias="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOptionTranslation\ConfigurationGroupOptionTranslationRepository.v1" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOptionTranslation\ConfigurationGroupOptionTranslationRepository" public="true" />
        <service alias="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupTranslation\ConfigurationGroupTranslationRepository.v1" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupTranslation\ConfigurationGroupTranslationRepository" public="true" />

         <service class="Shopware\Core\Content\Configuration\ConfigurationGroupDefinition" id="Shopware\Core\Content\Configuration\ConfigurationGroupDefinition" >
            <tag name="shopware.entity.definition" entity="configuration_group" />
        </service>

        <service class="Shopware\Core\Content\Configuration\ConfigurationGroupRepository" id="Shopware\Core\Content\Configuration\ConfigurationGroupRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>

         <service class="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOption\ConfigurationGroupOptionDefinition" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOption\ConfigurationGroupOptionDefinition" >
            <tag name="shopware.entity.definition" entity="configuration_group_option" />
        </service>

        <service class="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOption\ConfigurationGroupOptionRepository" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOption\ConfigurationGroupOptionRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>

         <service class="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOptionTranslation\ConfigurationGroupOptionTranslationDefinition" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOptionTranslation\ConfigurationGroupOptionTranslationDefinition" >
            <tag name="shopware.entity.definition" entity="configuration_group_option_translation" />
        </service>

        <service class="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOptionTranslation\ConfigurationGroupOptionTranslationRepository" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupOptionTranslation\ConfigurationGroupOptionTranslationRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>

         <service class="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupTranslation\ConfigurationGroupTranslationDefinition" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupTranslation\ConfigurationGroupTranslationDefinition" >
            <tag name="shopware.entity.definition" entity="configuration_group_translation" />
        </service>

        <service class="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupTranslation\ConfigurationGroupTranslationRepository" id="Shopware\Core\Content\Configuration\Aggregate\ConfigurationGroupTranslation\ConfigurationGroupTranslationRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                