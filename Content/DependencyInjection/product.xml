        <service class="Shopware\Core\Content\Product\Aggregate\ProductConfigurator\ProductConfiguratorRepository" id="Shopware\Core\Content\Product\Aggregate\ProductConfigurator\ProductConfiguratorRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Core\Content\Product\Aggregate\ProductContextPrice\ProductContextPriceDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductContextPrice\ProductContextPriceDefinition" >
