        <service class="Shopware\Core\Content\Product\Aggregate\ProductCategoryTree\ProductCategoryTreeDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductCategoryTree\ProductCategoryTreeDefinition" >
            <tag name="shopware.entity.definition" entity="product_category_tree" />
        </service>
        <service class="Shopware\Core\Content\Product\Util\VariantGenerator" id="Shopware\Core\Content\Product\Util\VariantGenerator" public="true">
            <argument id="Shopware\Core\Content\Product\ProductRepository" type="service"/>
            <argument id="Shopware\Core\Content\Product\Aggregate\ProductConfigurator\ProductConfiguratorRepository" type="service"/>
        </service>

        <!--cart implementations-->
        <service class="Shopware\Core\Content\Product\Cart\ProductCartCollector" id="Shopware\Core\Content\Product\Cart\ProductCartCollector">
            <argument id="Shopware\Core\Content\Product\Cart\ProductGateway" type="service"/>
            <tag name="cart.collector" priority="1000" />
            <argument type="service" id="Shopware\Core\Content\Product\Aggregate\ProductService\ProductServiceRepository"/>
        </service>

        <service id="Shopware\Core\Content\Product\Cart\ProductGateway" class="Shopware\Core\Content\Product\Cart\ProductGateway" >
            <argument type="service" id="Shopware\Core\Content\Product\ProductRepository"/>
        </service>

        <service class="Shopware\Core\Content\Product\Cart\ProductCalculator" id="Shopware\Core\Content\Product\Cart\ProductCalculator">
            <argument id="Shopware\Core\Checkout\Cart\Price\PriceCalculator" type="service"/>
        </service>

        <service class="Shopware\Core\Content\Product\Cart\ProductProcessor" id="Shopware\Core\Content\Product\Cart\ProductProcessor">
            <argument type="service" id="Shopware\Core\Content\Product\Cart\ProductCalculator" />
            <tag name="cart.processor" priority="1000" />
        </service>

        <service class="Shopware\Core\Content\Product\Util\KeywordSearchTermInterpreter" id="Shopware\Core\Content\Product\Util\KeywordSearchTermInterpreter">
            <argument type="service" id="Doctrine\DBAL\Connection"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\Term\Tokenizer"/>
            <argument type="service" id="logger"/>
        </service>

