        </service>
         <service class="Shopware\Core\Content\Product\Aggregate\ProductManufacturer\ProductManufacturerDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductManufacturer\ProductManufacturerDefinition" >
            <tag name="shopware.entity.definition" entity="product_manufacturer" />
        </service>
        <service class="Shopware\Core\Content\Product\Aggregate\ProductManufacturer\ProductManufacturerRepository" id="Shopware\Core\Content\Product\Aggregate\ProductManufacturer\ProductManufacturerRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
