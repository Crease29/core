        <service class="Shopware\Core\Content\Product\StorefrontProductRepository" id="Shopware\Core\Content\Product\StorefrontProductRepository">
            <argument type="service" id="Shopware\Core\Content\Product\ProductRepository"/>
            <argument type="service" id="Shopware\Core\Checkout\Cart\Price\PriceCalculator"/>
            <tag name="shopware.traceable" label="product.storefront-repository" />
        </service>
        <service class="Shopware\Core\Content\Product\StorefrontProductController" id="Shopware\Core\Content\Product\StorefrontProductController" public="true">
            <argument id="Shopware\Core\Content\Product\StorefrontProductRepository" type="service"/>
            <argument type="service" id="Shopware\Core\Framework\Api\Response\ResponseFactory"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\SearchCriteriaBuilder"/>
        </service>

        <service class="Shopware\Core\Content\Product\ProductDefinition" id="Shopware\Core\Content\Product\ProductDefinition" >
            <tag name="shopware.entity.definition" entity="product" />
        </service>
        <service class="Shopware\Core\Content\Product\ProductRepository" id="Shopware\Core\Content\Product\ProductRepository.v1" public="true">
