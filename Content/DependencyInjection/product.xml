        </service>
        <service class="Shopware\Core\Content\Product\Aggregate\ProductContextPrice\ProductContextPriceRepository" id="Shopware\Core\Content\Product\Aggregate\ProductContextPrice\ProductContextPriceRepository.v1" public="true">
