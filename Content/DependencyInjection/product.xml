        </service>
         <service class="Shopware\Core\Content\Product\Aggregate\ProductSearchKeyword\ProductSearchKeywordDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductSearchKeyword\ProductSearchKeywordDefinition" >
            <tag name="shopware.entity.definition" entity="product_search_keyword" />
        </service>
        <service class="Shopware\Core\Content\Product\Aggregate\ProductSearchKeyword\ProductSearchKeywordRepository" id="Shopware\Core\Content\Product\Aggregate\ProductSearchKeyword\ProductSearchKeywordRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Version\Service\VersionManager" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Core\Content\Product\Aggregate\ProductSeoCategory\ProductSeoCategoryDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductSeoCategory\ProductSeoCategoryDefinition" >
            <tag name="shopware.entity.definition" entity="product_seo_category" />
        </service>
         <service class="Shopware\Core\Content\Product\Aggregate\ProductService\ProductServiceDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductService\ProductServiceDefinition" >
