        </service>
        <service class="Shopware\Core\Content\Product\Aggregate\ProductManufacturerTranslation\ProductManufacturerTranslationRepository" id="Shopware\Core\Content\Product\Aggregate\ProductManufacturerTranslation\ProductManufacturerTranslationRepository.v1" public="true">
          <argument id="Shopware\Core\Framework\ORM\Dbal\EntityReader" type="service"/>
