        </service>
         <service class="Shopware\Core\Content\Product\Aggregate\ProductConfigurator\ProductConfiguratorDefinition" id="Shopware\Core\Content\Product\Aggregate\ProductConfigurator\ProductConfiguratorDefinition" >
