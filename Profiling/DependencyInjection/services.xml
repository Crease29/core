<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Shopware\Core\Profiling\DataCollector\CartCollector" public="false">
            <tag name="data_collector" template="@Profiling/Profiler/layout.html.twig" id="cart"/>
            <argument type="service" id="Shopware\Core\Profiling\Cart\TracedCartActions"/>
        </service>

        <service class="Shopware\Core\Profiling\Cart\TracedCartActions" id="Shopware\Core\Profiling\Cart\TracedCartActions"/>

        <service class="Shopware\Core\Profiling\Dbal\TraceableEntityReader"
                 id="Shopware\Core\Profiling\Dbal\TraceableEntityReader"
                 decorates="Shopware\Core\Framework\ORM\Read\EntityReaderInterface">

            <argument id="Shopware\Core\Profiling\Dbal\TraceableEntityReader.inner" type="service"/>
            <argument id="debug.stopwatch" type="service"/>
        </service>

        <service class="Shopware\Core\Profiling\Dbal\TraceableEntitySearcher"
                 id="Shopware\Core\Profiling\Dbal\TraceableEntitySearcher"
                 decorates="Shopware\Core\Framework\ORM\Search\EntitySearcherInterface">

            <argument id="Shopware\Core\Profiling\Dbal\TraceableEntitySearcher.inner" type="service"/>
            <argument id="debug.stopwatch" type="service"/>
        </service>
    </services>
</container>