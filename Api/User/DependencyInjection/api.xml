<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Versioning -->
        <service alias="Shopware\Api\User\Repository\UserRepository.v1" id="Shopware\Api\User\Repository\UserRepository" public="true"/>

        <service class="Shopware\Api\User\Definition\UserDefinition" id="shopware.user.user_definition" >
            <tag name="shopware.entity.definition" entity="user" />
        </service>
        <service class="Shopware\Api\User\Repository\UserRepository" id="Shopware\Api\User\Repository\UserRepository.v1" public="true">
          <argument id="Shopware\Api\Entity\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Version\VersionManager" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Api\Entity\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                